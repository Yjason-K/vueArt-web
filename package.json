    "eslint": "9.21.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "15.15.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "typescript": "5.7.2",
    "typescript-eslint": "8.24.1",
    "vite": "6.2.0",
    "vite-tsconfig-paths": "5.1.4"
